<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
    <title>
      {{ page_title }}
      {%- if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
      {%- if current_page != 1 %} &ndash; Page {{ current_page }}{% endif -%}
      {%- unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless -%}
    </title>
    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    {% render 'meta-tags' %}

<script>
    window.data = window.data || {};
    window.data.shop = {{ shop |json }};
    window.data.cart = {{ cart | json }};
    window.data.customer = {{ customer | json }}
  </script>

{% case template.name %}
{% when  'product' %}
  <script>
     window.data = window.data || {};
    window.data.product = {{ product | json }};
   window.data.metafields = 
    {{ product.metafields.custom ||json}}
  ;
  </script>
{% when 'collection' %}
<script>
    window.data = window.data || {};
    window.data.collection = {{ collection | json }};
  </script>
{% when 'blog' %}
<script>
    window.data = window.data || {};
    window.data.blog = {{ blog | json }};
  </script>
{% when 'article' %}
<script>
    window.data = window.data || {};
    window.data.article = {{ article | json }};
  </script>
{% when 'search' %}
<script>
    window.data = window.data || {};
    window.data.search =  [{{ search.results }},{{ search.terms }}] 
   
  </script>

{% endcase %}
    
    {{ content_for_header }}
  {% if request.design_mode %}
<script async src="https://cdn.builder.io/js/webcomponents"></script>
{% endif %}
        {% render 'builder-head' %}
</head>



  <body>
{% comment %}
  Accouncement bar installed by Builder.io
  You may modify this code at any time
{% endcomment %}
{% include 'model.announcement-bar.builder' %}


    <main role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>
  </body>
</html>
